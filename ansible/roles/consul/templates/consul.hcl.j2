datacenter = "dc1"
data_dir = "/opt/consul"
log_level = "DEBUG"
node_name = "{{ inventory_hostname }}"
server = true
bootstrap_expect = 3

bind_addr = "{{ ansible_default_ipv4.address }}"
client_addr = "0.0.0.0"

retry_join = ["192.168.0.2", "192.168.0.3", "192.168.0.4"]

ui_config {
    enabled = true
}